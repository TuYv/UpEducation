{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "升本查询"
      }
    },
    {
      "path": "pages/mine/mine",
      "style": {
        "navigationBarTitleText": "我的"
      }
    }
  ],
  "tabBar": {
    "color": "#b3b3b3",
    "selectedColor": "#38c4ae",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "static/images/search.png",
        "selectedIconPath": "static/images/search_active.png",
        "text": "升本查询"
      },
      {
        "pagePath": "pages/mine/mine",
        "iconPath": "static/images/mine.png",
        "selectedIconPath": "static/images/mine_active.png",
        "text": "我的"
      }
    ]
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#38c4ae",
    "backgroundColor": "#f6f6f6"
  }
}
